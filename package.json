{
  "name": "correios-brasil",
  "version": "1.0.4",
  "description": "Módulo completo consultar informações sobre o CEP, calcular o preço e os prazos das entregas das encomendas e também realizar o seu rastreio",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "ts-node index.ts",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts",
    "declarations": "tsc -emitDeclarationOnly --d",
    "build": "rimraf dist && mkdirp dist && rollup -c",
    "postbuild": "dts-bundle-generator -o ./dist/corrreios.d.ts ./src/index.ts"
  },
  "keywords": [
    "entrega",
    "entregas",
    "preço",
    "prazo",
    "correio",
    "correios",
    "frete",
    "rastreio",
    "rastrear",
    "encomendas",
    "encomenda",
    "cep",
    "ibge",
    "viacep",
    "soap",
    "xml",
    "json",
    "de"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FinotiLucas/Correios-Brasil"
  },
  "author": "Lucas Lima Finoti <lucas.finoti@protonmail.com>",
  "license": "Apache License 2.0",
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "esm": "^3.2.25",
    "iconv-lite": "^0.5.1",
    "node-fetch": "^2.6.0",
    "xml-js": "^1.6.11",
    "@types/cheerio": "^0.22.18",
    "@types/node-fetch": "^2.5.7",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "chai": "^4.2.0",
    "dts-bundle-generator": "^4.3.0",
    "esm": "^3.2.25",
    "mkdirp": "^1.0.4",
    "mocha": "^7.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.10.5",
    "rollup-plugin-dts": "^1.4.6",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-node": "^8.10.1",
    "tslib": "^2.0.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  }
}
